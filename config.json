{
  "system": {
    "name": "Expert Pancake AI System",
    "version": "1.0.0"
  },
  "email_handler": {
    "max_emails": 50,
    "batch_size": 10,
    "categories": ["urgent", "important", "normal", "low_priority"],
    "auto_response": true
  },
  "security_manager": {
    "enable_2fa": true,
    "session_timeout": 3600,
    "encryption_enabled": true,
    "access_levels": ["admin", "user", "guest"]
  },
  "ai_coordinator": {
    "concurrent_tasks": 5,
    "retry_attempts": 3,
    "logging_enabled": true
  }
}
